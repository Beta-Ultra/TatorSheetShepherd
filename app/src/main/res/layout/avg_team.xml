<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/teamNumber"
        android:text="Team Number"
        android:layout_width="0dp"
        android:layout_weight="0.08"
        android:layout_height="wrap_content"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/lowBarX"
        android:text="LowBarX"
        android:layout_width="0dp"
        android:layout_weight="0.08"
        android:layout_height="wrap_content"
        android:textColor="#999999" />

    <TextView
        android:id="@+id/lowBarT"
        android:text="LowBarTime"
        android:layout_width="0dp"
        android:layout_weight="0.08"
        android:layout_height="wrap_content"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/lowF"
        android:text="LowFails"
        android:layout_width="0dp"
        android:layout_weight="0.08"
        android:layout_height="wrap_content"
        android:textColor="#999999"/>

    <TextView
        android:id="@+id/chevalX"
        android:text="ChevalX"
        android:layout_width="0dp"
        android:layout_weight="0.07"
        android:layout_height="wrap_content"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/chevalT"
        android:text="ChevalTime"
        android:layout_width="0dp"
        android:layout_weight="0.08"
        android:layout_height="wrap_content"
        android:textColor="#999999"/>

    <TextView
        android:id="@+id/chevalF"
        android:text="ChevalFails"
        android:layout_width="0dp"
        android:layout_weight="0.08"
        android:layout_height="wrap_content"
        android:textColor="#000000"/>
</LinearLayout>